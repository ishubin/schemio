{
    "name": "Tabs panel",
    "args": {
        "numTabs": {"type": "number", "value": 3, "min": 1, "name": "Numbers of tabs"},
        "tabWidth": {"type": "number", "value": 100, "min": 0, "name": "Tab width"},
        "tabHeight": {"type": "number", "value": 40, "min": 0, "name": "Tab height"},
        "tabCornerRadius": {"type": "number", "value": 6, "min": 0, "name": "Tab Corner radius"},
        "cornerRadius": {"type": "number", "value": 10, "min": 0, "name": "Corner radius"},
        "tabGap": {"type": "number", "value": 5, "min": 0, "name": "Tab gap", "description": "Distance between tabs"},
        "padding": {"type": "number", "value": 10, "min": 0, "name": "Padding"},
        "tabBackground": {"type": "advanced-color", "value": { "type": "solid", "color": "rgba(255, 255, 255, 1)" }, "name": "Tab background"},
        "inactiveBackground": {"type": "advanced-color", "value": { "type": "solid", "color": "rgba(232, 232, 232, 1)" }, "name": "Inactive tab background"},
        "background": {"type": "advanced-color", "value": { "type": "solid", "color": "rgba(240, 240, 240, 1)" }, "name": "Background"},
        "strokeColor": {"type": "color", "value": "rgba(212,212,212,1.0)", "name": "Stroke color"},
        "textColor": {"type": "color", "value": "rgba(30,30,30,1.0)", "name": "Text color"},
        "shadow": {"type": "boolean", "value": true, "name": "Drop shadow"},
        "showAllTabs": {"type": "boolean", "value": false, "name": "Show all tabs", "description": "Displays all tabs in edit mode next to each other for easier editing. In view mode all the tabs will be placed in their proper position and will be hidden"}
    },
    "init": [
        "topMargin = padding",
        "tabMargin = max(10, cornerRadius) + padding",
        "tw = max(10, min((width - 2 * tabMargin) / numTabs - tabGap, tabWidth))",
        "tabTag = 'tab-' + uid()",
        "containerTag = 'tab-container-' + uid()",
        "containerWidth = max(1, width - padding * 2)"
    ],
    "controls": [ {
        "name": "addTab",
        "type": "button",
        "text": "New tab",
        "x": {"$-expr": "tabMargin + (numTabs) * (tw + tabGap) + max(15, tabGap * 2)"},
        "y": {"$-expr": "topMargin"},
        "width": 60,
        "height": 30,
        "click": [
            "numTabs = numTabs + 1"
        ]
    }, {
        "$-if": "numTabs > 1",
        "name": "removeTab",
        "type": "button",
        "text": "â¤«",
        "x": {"$-expr": "tabMargin + (numTabs - 1) * (tw + tabGap) + tw - 10"},
        "y": {"$-expr": "topMargin - 10"},
        "width": 20,
        "height": 20,
        "click": [
            "numTabs = max(1, numTabs - 1)"
        ]
    }, {
        "$-if": "showAllTabs == true",
        "type": "button",
        "text": "Hide all tabs",
        "x": {"$-expr": "width"},
        "y": {"$-expr": "0"},
        "width": 90,
        "height": 30,
        "click": [
            "showAllTabs = false"
        ]
    }, {
        "$-else": "",
        "type": "button",
        "text": "Show all tabs",
        "x": {"$-expr": "width"},
        "y": {"$-expr": "0"},
        "width": 90,
        "height": 30,
        "click": [
            "showAllTabs = true"
        ]
    }],
    "description": "Panel with tabs",
    "preview": "data:image/svg+xml;base64,",
    "item": {
        "id": "root",
        "shapeProps": {
            "cornerRadius": {"$-expr": "cornerRadius"},
            "strokeColor": {"$-expr": "strokeColor"},
            "strokeSize": 1,
            "fill": {"$-expr": "background"},
            "strokePattern": "solid"
        },
        "shape": "rect",
        "area": {
            "x": 0,
            "y": 0,
            "w": 540,
            "h": 360,
            "r": 0,
            "px": 0.5,
            "py": 0.5,
            "sx": 1,
            "sy": 1
        },
        "name": "Tabs panel",
        "links": [],
        "behavior": {
            "events": [
                {
                    "id": {"$-expr": "uid()"},
                    "event": "init",
                    "actions": [
                        {
                            "id": {"$-expr": "uid()"},
                            "element": {"$-str": "tag: ${tabTag}"},
                            "method": "set",
                            "on": true,
                            "args": {
                                "field": "shapeProps.fill",
                                "value": {"$-expr": "inactiveBackground"},
                                "animated": false,
                                "animationDuration": 0.2,
                                "transition": "ease-in-out",
                                "inBackground": true
                            }
                        },
                        {
                            "id": {"$-expr": "uid()"},
                            "element": "#tab-1",
                            "method": "set",
                            "on": true,
                            "args": {
                                "field": "shapeProps.fill",
                                "value": {"$-expr": "tabBackground"},
                                "animated": false,
                                "animationDuration": 0.2,
                                "transition": "ease-in-out",
                                "inBackground": true
                            }
                        },
                        {
                            "id": {"$-expr": "uid()"},
                            "element": "#tab-container-1",
                            "method": "toggleGroup",
                            "on": true,
                            "args": {
                                "group": {"$-str": "tag: ${containerTag}"}
                            }
                        }
                    ]
                }
            ]
        },
        "tags": [],
        "effects": [
            {
                "$-if": "shadow == true",
                "id": "Lira_Np5T",
                "effect": "drop-shadow",
                "name": "Drop Shadow",
                "args": {
                    "color": "rgba(0,0,0,1.0)",
                    "dx": 3,
                    "dy": 3,
                    "blur": 20,
                    "opacity": 20,
                    "inside": false
                }
            }
        ],
        "childItems": [
            {
                "$-for": {"start": 0, "until": {"$-expr": "numTabs"}, "it": "idx"},
                "id": {"$-str": "tab-${idx+1}"},
                "name": {"$-str": "Tab ${idx+1}"},
                "shapeProps": {
                    "cornerRadius": {"$-expr": "tabCornerRadius"},
                    "fill": {"$-expr": "ifcond(idx == 0, tabBackground, inactiveBackground)"},
                    "strokeColor": "rgba(30,30,30,1.0)",
                    "strokeSize": 0,
                    "strokePattern": "solid"
                },
                "shape": "rect",
                "cursor": "pointer",
                "area": {
                    "x": {"$-expr": "tabMargin + idx * (tw + tabGap)"},
                    "y": {"$-expr": "topMargin"},
                    "w": {"$-expr": "tw"},
                    "h": {"$-expr": "tabHeight"},
                    "r": 0,
                    "px": 0.5,
                    "py": 0.5,
                    "sx": 1,
                    "sy": 1
                },
                "opacity": 100,
                "blendMode": "normal",
                "description": "",
                "links": [],
                "textSlots": {
                    "body": {
                        "text": {"$-str": "<p>Tab ${idx+1}</p>"},
                        "color": {"$-expr": "textColor"},
                        "halign": "center",
                        "valign": "top",
                        "fontSize": 14,
                        "whiteSpace": "normal",
                        "font": "Arial",
                        "paddingLeft": 0,
                        "paddingRight": 0,
                        "paddingTop": 10,
                        "paddingBottom": 0
                    }
                },
                "behavior": {
                    "events": [
                        {
                            "id": {"$-expr": "uid()"},
                            "event": "clicked",
                            "actions": [
                                {
                                    "id": {"$-expr": "uid()"},
                                    "element": {"$-str": "tag: ${tabTag}"},
                                    "method": "set",
                                    "on": true,
                                    "args": {
                                        "field": "shapeProps.fill",
                                        "value": {"$-expr": "inactiveBackground"},
                                        "animated": false,
                                        "animationDuration": 0.2,
                                        "transition": "ease-in-out",
                                        "inBackground": true
                                    }
                                },
                                {
                                    "id": {"$-expr": "uid()"},
                                    "element": "self",
                                    "method": "set",
                                    "on": true,
                                    "args": {
                                        "field": "shapeProps.fill",
                                        "value": {"$-expr": "tabBackground"},
                                        "animated": false,
                                        "animationDuration": 0.2,
                                        "transition": "ease-in-out",
                                        "inBackground": true
                                    }
                                },
                                {
                                    "id": {"$-expr": "uid()"},
                                    "element": {"$-str": "#tab-container-${idx+1}"},
                                    "method": "toggleGroup",
                                    "on": true,
                                    "args": {
                                        "group": {"$-str": "tag: ${containerTag}"}
                                    }
                                }
                            ]
                        }
                    ]
                },
                "selfOpacity": 100,
                "visible": true,
                "tags": [ {"$-expr": "tabTag"} ],
                "clip": false,
                "effects": [],
                "interactionMode": "tooltip",
                "tooltipBackground": "rgba(230,230,230,1.0)",
                "tooltipColor": "rgba(30,30,30,1.0)",
                "locked": true
            },
            {
                "$-for": {"start": 0, "until": {"$-expr": "numTabs"}, "it": "idx"},
                "id": {"$-str": "tab-container-${idx+1}"},
                "shapeProps": {
                    "cornerRadius": {"$-expr": "cornerRadius"},
                    "fill": {"$-expr": "tabBackground"},
                    "strokeColor": "rgba(255, 255, 255, 1)",
                    "strokeSize": 0,
                    "strokePattern": "solid"
                },
                "shape": "rect",
                "cursor": "default",
                "area": {
                    "x": {"$-expr": "ifcond(showAllTabs, padding + idx * (containerWidth + padding) + ifcond(idx > 0, 2*padding, 0), padding)"},
                    "y": {"$-expr": "topMargin + tabHeight - tabCornerRadius - 1"},
                    "w": {"$-expr": "containerWidth"},
                    "h": {"$-expr": "max(1, height - padding - (topMargin + tabHeight - tabCornerRadius - 1))"},
                    "r": 0,
                    "px": 0.5,
                    "py": 0.5,
                    "sx": 1,
                    "sy": 1
                },
                "opacity": 100,
                "blendMode": "normal",
                "name": {"$-str": "Tab container ${idx+1}"},
                "description": "",
                "links": [],
                "textSlots": {
                    "body": {
                        "text": {"$-str": "<h2>Tab ${idx+1}</h2><p><b>Attach items here</b></p><p><i>(This text will be hidden in view mode)</i></p>"},
                        "color": "rgba(150,150,150,1.0)",
                        "halign": "center",
                        "valign": "middle",
                        "fontSize": 16,
                        "whiteSpace": "normal",
                        "font": "Arial",
                        "paddingLeft": 0,
                        "paddingRight": 0,
                        "paddingTop": 0,
                        "paddingBottom": 0
                    }
                },
                "behavior": {
                    "events": [{
                        "id": {"$-expr": "uid()"},
                        "event": "init",
                        "actions": [{
                            "id": {"$-expr": "uid()"},
                            "on": true,
                            "element": "self",
                            "method": "set",
                            "args": {
                                "field": "textSlots.body.color",
                                "value": "rgba(0,0,0,0.0)"
                            }
                        }, {
                            "id": {"$-expr": "uid()"},
                            "on": true,
                            "element": "self",
                            "method": "move",
                            "args": {
                                "x": {"$-expr": "padding"},
                                "y": {"$-expr": "topMargin + tabHeight - tabCornerRadius - 1"},
                                "animate": false
                            }
                        }]
                    }]
                },
                "selfOpacity": 100,
                "visible": {"$-expr": "ifcond(showAllTabs, true, idx == 0"},
                "tags": [
                    {"$-expr": "containerTag"}
                ],
                "clip": false,
                "effects": [],
                "interactionMode": "tooltip",
                "tooltipBackground": "rgba(230,230,230,1.0)",
                "tooltipColor": "rgba(30,30,30,1.0)",
                "locked": true
            }
        ],
        "description": "",
        "textSlots": {
            "body": {
                "text": "",
                "color": "rgba(0,0,0,1.0)",
                "halign": "center",
                "valign": "middle",
                "fontSize": 14,
                "whiteSpace": "normal",
                "font": "Arial",
                "paddingLeft": 0,
                "paddingRight": 0,
                "paddingTop": 0,
                "paddingBottom": 0
            }
        },
        "opacity": 100,
        "selfOpacity": 100,
        "visible": true,
        "blendMode": "normal",
        "cursor": "default",
        "clip": false,
        "interactionMode": "tooltip",
        "tooltipBackground": "rgba(230,230,230,1.0)",
        "tooltipColor": "rgba(30,30,30,1.0)"
    }
}