{
  "name": "schemio",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "NODE_ENV=development nodemon --watch src/  -e 'js, html, vue' --exec 'npm run build-dev; babel-node src/server/server.js'",
    "build-clean-init": "mkdir -p dist; rm -rf dist/*; mkdir -p dist/server; mkdir -p uploads",
    "build-assets": "npm run build-clean-init; mkdir -p dist/assets; cp -r assets/* dist/assets/; node src/build/buildHTML.js",
    "build-dev": "webpack --config webpack.build.dev.js; npm run build-standalone",
    "build-prod": "npm run build-assets; webpack --config webpack.build.prod.js; npm run build-standalone-prod",
    "build-standalone": "webpack --config webpack.standalone.js",
    "build-standalone-prod": "webpack --config webpack.standalone.prod.js",
    "test": "mocha-webpack --webpack-config webpack.build.test.js -r test/setup.js test/*.spec.js test/**/*.spec.js"
  },
  "author": "Ivan Shubin",
  "license": "MPL-2.0",
  "devDependencies": {
    "@johmun/vue-tags-input": "^1.5.10",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.21.1",
    "babel-cli": "^6.25.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "expect": "^24.8.0",
    "file-loader": "^0.11.2",
    "jsdom": "^16.4.0",
    "jszip": "^3.5.0",
    "mocha": "^7.0.0",
    "mocha-webpack": "v2.0.0-beta.0",
    "sanitize-html": "^1.20.1",
    "shortid": "^2.2.14",
    "tiptap": "^1.22.3",
    "tiptap-extensions": "^1.22.3",
    "vue": "^2.6.10",
    "vue-axios": "^2.0.2",
    "vue-color": "2.7.0",
    "vue-gtag": "^1.16.1",
    "vue-loader": "^15.8.3",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.5.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fs-extra": "^5.0.0",
    "multer": "^1.4.1",
    "nodemon": "^1.18.7",
    "properties-reader": "0.0.16",
    "sinon": "^7.4.2"
  }
}
